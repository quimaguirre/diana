scp -r /home/quim/PHD/Projects/DIANA/diana quim@hydra:/users/sbi/quim/DIANA
scp -r /home/quim/PHD/Projects/DIANA/diana/scripts/* quim@hydra:/users/sbi/quim/DIANA/diana/scripts
